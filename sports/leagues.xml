<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd" securityLevel="any">
	 <a class="link" href="yql-opentables-reference.html#yql-opentables-meta" title="meta element">
		<meta></a>
			<author>Tadeu Luis Pires Gaudio</author>
			<documentationURL>http://td2-cdn.appspot/services/api/sports.leagues.html</documentationURL>  
			<sampleQuery>select * from {table} where league="true" and text="san francisco"</sampleQuery>  
			<description>Allow access a sports data.</description>
	  <a class="link" href="yql-opentables-reference.html#yql-opentables-meta" title="meta element">
	  	 </meta></a>
	<bindings>
		<select itemPath="rsp.leagues" produces="XML">
			<urls>
				<url env="all">http://td2-cdn.appspot.com/services/rest/?method=sports.leagues</url>
			</urls>
			<paging model="page">
				<start id="page" default="0" />
				<pagesize id="per_page" max="250" />
				<total default="10" />
			</paging>
			<inputs>
				<key id="leagues_id" type="xs:string" paramType="query" />
				<key id="leagues_name" type="xs:string" paramType="query" />  
				<key id="leagues_year" type="xs:string" paramType="query" />  
				<key id="leagues_start" type="xs:string" paramType="query" />
			</inputs>
		</select>
	</bindings>
</table>  
