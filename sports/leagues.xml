<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd" securityLevel="any">
		<meta>
			<author>Tadeu Luis Pires Gaudio</author>
			<documentationURL>http://td2-cdn.appspot/services/api/sports.leagues.html</documentationURL>  
			<sampleQuery>select * from {table} where leagues_year=2010"</sampleQuery>  
			<description>Allow access a sports data.</description>
	  	 </meta>
	<bindings>
		<select itemPath="" produces="XML">
			<urls>
				<url></url>
			</urls>
			<paging model="page">
				<start id="page" default="0" />
				<pagesize id="per_page" max="250" />
				<total default="10" />
			</paging>
			<inputs>
				<key id="leagues_id" type="xs:string" paramType="query" />
				<key id="leagues_name" type="xs:string" paramType="query" />  
				<key id="leagues_year" type="xs:string" paramType="query" />  
				<key id="leagues_start" type="xs:string" paramType="query" />
                <key id="api_key" type="xs:string" const="true" private="true" paramType="query" default="anestesya@gmail.com" />  
			</inputs>
			<execute>
				<![CDATA[
				    var querystring = 'select * from html where url="http://www.soccerway.com/competitions/" and xpath='+"'//div[@class="+ '"col-50-r"]'+"'"; 
					query = y.query(querystring);
					response.object = query.results; 
				]]>
			</execute>
		</select>
	</bindings>
</table>  